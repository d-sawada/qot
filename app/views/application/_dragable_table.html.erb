<table class="table dragable">
  <thead>
    <tr>
      <% @table_keys.each do |key| %>
        <th scope="col"><%= key %><span/></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @table_rows.each do |row| %>
      <tr>
        <% row.each do |val| %>
          <td><%= val %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
